    "tokenColors": [{
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#666666",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Keywords and Control Flow",
            "scope": [
                "keyword",
                "keyword.control",
                "keyword.operator.new",
                "keyword.control.flow",
                "keyword.control.import",
                "keyword.control.export",
                "keyword.control.from",
                "keyword.control.as",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#ff00ff",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Python Keywords",
            "scope": [
                "keyword.control.python",
                "keyword.control.flow.python",
                "keyword.control.import.python",
                "keyword.operator.logical.python"
            ],
            "settings": {
                "foreground": "#ff00ff",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Python Decorators",
            "scope": [
                "entity.name.function.decorator.python",
                "meta.function.decorator.python",
                "punctuation.definition.decorator.python"
            ],
            "settings": {
                "foreground": "#ff0080",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Python Self/Cls",
            "scope": [
                "variable.language.special.self.python",
                "variable.parameter.function.language.special.self.python",
                "variable.language.special.cls.python"
            ],
            "settings": {
                "foreground": "#ff6600",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Python Built-in Functions",
            "scope": [
                "support.function.builtin.python",
                "support.function.magic.python"
            ],
            "settings": {
                "foreground": "#4444dd",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Python Class Names",
            "scope": [
                "entity.name.type.class.python",
                "support.type.python"
            ],
            "settings": {
                "foreground": "#ff0080",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Python Function Definitions",
            "scope": [
                "entity.name.function.python",
                "meta.function.python"
            ],
            "settings": {
                "foreground": "#4444dd",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Python Function Parameters",
            "scope": [
                "variable.parameter.function.python"
